<h1>notifications</h1>
<% for (let i = notifications.length-1; i >= 0; i--) {  %>
  ID: <%= notifications[i]._id %><br>
  Title: <%= notifications[i].title %><br>
  Text: <%= notifications[i].text %><br>
  <form method="post" accept-charset="UTF-8">
    <input type="hidden" name="_csrf" value='<%=csrfToken%>'>
    <input type="hidden" name="id" value="<%=notifications[i]._id%>">
    <input class="signupBtn" type="submit" name="search" value="delete">
  </form>
  <br><br>
<%}%>
