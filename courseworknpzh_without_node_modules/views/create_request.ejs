<h1>Create request</h1>
<form method="post" accept-charset="UTF-8">
  <div id="regInsideContent">
    <div class="regField">
      <label>Title:</label>
      <input required type="text" name="title">
    </div>
    <div class="regField">
      <label>Text:</label>
      <input required type="text" name="text">
    </div>
    <div class="regField">
      <label>Category:</label>
      <input required type="text" name="category">
    </div>
  </div>
  <input type="hidden" name="_csrf" value='<%=csrfToken%>'>
  <div class="regField">
    <input id="submitButt" class="signupBtn" type="submit" value="Sign Me Up!">
  </div>
</form>
